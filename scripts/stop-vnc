#! /bin/bash

echo
echo Stopping Remote Desktop $1 on $(hostname)
echo
echo vncserver -kill :$1
vncserver -kill :$1
sleep 1

echo
echo
echo
echo "A window should now pop up saying that you have been Disconnected."
echo
echo "Please use the 'Home' button to return to the main menu."
echo
echo
sleep 3
echo
